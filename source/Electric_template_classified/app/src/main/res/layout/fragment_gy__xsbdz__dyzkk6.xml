        </LinearLayout>
    <!-- 型号 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_DYZKK6XH"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessSpinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK6XH"
        android:tag="@string/GY_XSBYQTZXX_F_DYZKK6XH"/>


    <!-- 生产厂家 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_DYZKK6SCCJ"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
