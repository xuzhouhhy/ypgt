        android:id="@+id/F_DYZKK6SCCJ"
        android:tag="@string/GY_XSBYQTZXX_F_DYZKK6SCCJ"/>


    <!-- 投运日期 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_DYZKK6TYRQ"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK6TYRQ"
        android:tag="@string/GY_XSBYQTZXX_F_DYZKK6TYRQ"/>



    <!-- 额定电流(A) -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_DYZKK6EDDL"
