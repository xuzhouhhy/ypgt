        android:id="@+id/F_PDBYQKZSH"
        android:tag="@string/GY_XSBYQTZXX_F_PDBYQKZSH"/>


    <!-- 联接组标号  -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_PDBYQLJZBH"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
