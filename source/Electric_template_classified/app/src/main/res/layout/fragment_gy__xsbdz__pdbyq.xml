        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_PDBYQEDRL"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_PDBYQEDRL"
        android:tag="@string/GY_XSBYQTZXX_F_PDBYQEDRL"/>


    <!-- 短路损耗(W) -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_PDBYQDLSH"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
