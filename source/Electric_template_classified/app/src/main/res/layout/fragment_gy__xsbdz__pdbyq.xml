        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_PDBYQSFHW"
        android:tag="@string/GY_XSBYQTZXX_F_PDBYQSFHW"/>


    <!-- 绝缘介质 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_XSBYQTZXX_F_PDBYQJYJJ"
