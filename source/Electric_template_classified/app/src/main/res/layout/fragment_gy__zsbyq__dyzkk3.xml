<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayoutRoot">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal">
            <TextView
                style="@style/fragment_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/GY_ZSBYQ_TITLE_DYKK3"/>
        </LinearLayout>

    <!-- 型号 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_ZSBYQTZXX_F_DYZKK3XH"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessConcatSpinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK3XH"
        android:tag="@string/GY_ZSBYQTZXX_F_DYZKK3XH"/>


    <!-- 生产厂家 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_ZSBYQTZXX_F_DYZKK3SCCJ"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK3SCCJ"
        android:tag="@string/GY_ZSBYQTZXX_F_DYZKK3SCCJ"/>




    <!-- 投运日期 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_ZSBYQTZXX_F_DYZKK3TYRQ"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.EditTextDatetimeExpand
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK3TYRQ"
        android:tag="@string/GY_ZSBYQTZXX_F_DYZKK3TYRQ"/>


    <!-- 额定电流(A) -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_ZSBYQTZXX_F_DYZKK3EDDL"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK3EDDL"
        android:tag="@string/GY_ZSBYQTZXX_F_DYZKK3EDDL"/>




    <!-- 灭弧介质 -->
    <TextView
        style="@style/SimpleLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/GY_ZSBYQTZXX_F_DYZKK3MHJZ"
        />
    <com.geocraft.electrics.ui.view.UserDefineControlView.BusinessEditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/F_DYZKK3MHJZ"
        android:tag="@string/GY_ZSBYQTZXX_F_DYZKK3MHJZ"/>
    </LinearLayout>
</ScrollView>
